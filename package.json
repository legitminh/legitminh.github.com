        "eslint-config-next": "13.2.1",
        "next": "13.2.1",
        "pnpm": "^7.28.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "recoil": "^0.7.6",
        "typescript": "4.9.5"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.9",
        "autoprefixer": "^10.4.13",
        "postcss": "^8.4.21",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-preset-env": "^8.0.1",
        "sass": "^1.58.3",
